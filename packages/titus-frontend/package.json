{
  "name": "titus-frontend",
  "version": "1.0.0-alpha.1",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.10.3",
    "auth0-js": "^9.13.3",
    "aws-amplify": "^3.0.18",
    "cross-env": "^7.0.2",
    "env-schema": "^1.1.0",
    "fluent-schema": "^1.0.4",
    "formik": "^2.0.4",
    "i18next": "^19.5.1",
    "i18next-browser-languagedetector": "^4.3.1",
    "jwt-decode": "^2.2.0",
    "pino": "^6.3.2",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-adal": "^0.5.0",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.7.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.0",
    "regenerator-runtime": "^0.13.5",
    "yup": "^0.28.5"
  },
  "scripts": {
    "create:env": "node -r fs -e \"fs.copyFileSync('.env.sample', '.env', fs.constants.COPYFILE_EXCL)\"",
    "build": "react-app-rewired build",
    "eject": "react-app-rewired eject",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:staged": "lint-staged",
    "start": "npm run validate:env && react-app-rewired start",
    "storybook": "start-storybook -p 9009 -s public",
    "storybook:build": "build-storybook -s public",
    "test": "cross-env CI=true react-app-rewired test --env=jest-environment-jsdom-sixteen --watchAll=false --coverage  --colors",
    "test:local": "cross-env CI=false react-app-rewired test --env=jest-environment-jsdom-sixteen --watchAll=false --coverage  --colors",
    "test:watch": "react-app-rewired test --env=jest-environment-jsdom-sixteen",
    "test:related": "cross-env CI=true react-app-rewired test --env=jest-environment-jsdom-sixteen --onlyChanged",
    "validate:env": "node bin/validateEnv.js"
  },
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-syntax-optional-chaining": "^7.8.3",
    "@storybook/addon-a11y": "^5.3.19",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-docs": "^5.3.19",
    "@storybook/addon-info": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addon-options": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/preset-create-react-app": "^3.1.2",
    "@storybook/react": "^5.3.19",
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^10.4.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^4.0.0",
    "chalk": "^4.1.0",
    "customize-cra": "^1.0.0",
    "dotenv": "^8.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "puppeteer": "^3.3.0",
    "react-app-rewired": "^2.1.6",
    "react-is": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "serve-handler": "^6.1.3",
    "storybook-readme": "^5.0.8",
    "typescript": "^3.9.5"
  },
  "lint-staged": {
    "*.{js,css,md}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "~/(.*)$": "<rootDir>/src/$1"
    },
    "collectCoverageFrom": [
      "src/**/*js",
      "!**/__snapshots__",
      "!src/**/*.story.js",
      "!**/serviceWorker.js",
      "!src/index.js"
    ]
  },
  "proxy": "http://localhost:5000",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
