{
  "name": "titus-frontend",
  "version": "1.0.0-alpha.1",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.12.7",
    "auth0-js": "^9.14.0",
    "aws-amplify": "^3.3.9",
    "cross-env": "^7.0.2",
    "env-schema": "^2.0.1",
    "fluent-schema": "^1.0.5",
    "formik": "^2.2.5",
    "i18next": "^19.8.4",
    "i18next-browser-languagedetector": "^6.0.1",
    "jwt-decode": "^3.1.2",
    "pino": "^6.7.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-adal": "^0.5.0",
    "react-dom": "^17.0.1",
    "react-i18next": "^11.7.3",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.4",
    "regenerator-runtime": "^0.13.7",
    "yup": "^0.30.0"
  },
  "scripts": {
    "create:env": "node -r fs -e \"fs.copyFileSync('.env.sample', '.env', fs.constants.COPYFILE_EXCL)\"",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:staged": "lint-staged",
    "start": "npm run validate:env && react-scripts start",
    "storybook": "start-storybook -p 9009 -s public",
    "storybook:build": "build-storybook -s public",
    "test": "cross-env CI=true react-scripts test --env=jest-environment-jsdom-sixteen --watchAll=false --coverage  --colors",
    "test:local": "cross-env CI=false react-scripts test --env=jest-environment-jsdom-sixteen --watchAll=false --coverage  --colors",
    "test:watch": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "test:related": "cross-env CI=true react-scripts test --env=jest-environment-jsdom-sixteen --onlyChanged",
    "validate:env": "node bin/validateEnv.js",
    "postinstall": "patch-package"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^6.1.2",
    "@storybook/addon-actions": "^6.1.2",
    "@storybook/addon-docs": "^6.1.2",
    "@storybook/addon-knobs": "^6.1.2",
    "@storybook/addon-links": "^6.1.2",
    "@storybook/addons": "^6.1.2",
    "@storybook/preset-create-react-app": "^3.1.5",
    "@storybook/react": "^6.1.2",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "babel-loader": "^8.2.1",
    "chalk": "^4.1.0",
    "dotenv": "^8.2.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "lint-staged": "^10.5.1",
    "patch-package": "^6.2.2",
    "prettier": "^2.2.0",
    "pretty-quick": "^3.1.0",
    "puppeteer": "^5.5.0",
    "react-is": "^17.0.1",
    "react-test-renderer": "^17.0.1",
    "serve-handler": "^6.1.3",
    "storybook-readme": "^5.0.9",
    "typescript": "^4.1.2"
  },
  "lint-staged": {
    "*.{js,css,md}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*js",
      "!**/__snapshots__",
      "!src/**/*.story.js",
      "!**/serviceWorker.js",
      "!src/index.js"
    ]
  },
  "proxy": "http://localhost:5000",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
