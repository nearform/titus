{
  "name": "titus-frontend",
  "version": "1.0.0-alpha.1",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.9.0",
    "auth0-js": "^9.13.2",
    "aws-amplify": "^1.2.4",
    "cross-env": "^7.0.2",
    "formik": "^2.0.4",
    "pino": "^6.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-adal": "^0.5.0",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.0",
    "regenerator-runtime": "^0.13.5",
    "yup": "^0.28.4"
  },
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lighthouse": "node lighthouse",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:staged": "lint-staged",
    "start": "npm run validate:env && react-scripts start",
    "storybook": "start-storybook -p 9009 -s public",
    "storybook:build": "build-storybook -s public",
    "test": "cross-env CI=true react-scripts test --env=jest-environment-jsdom-sixteen --watchAll=false --coverage  --colors",
    "test:watch": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "test:related": "cross-env CI=true react-scripts test --env=jest-environment-jsdom-sixteen --onlyChanged",
    "validate:env": "node -r dotenv/config bin/validateEnv.js"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^5.3.18",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-info": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addon-options": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "@testing-library/react": "^10.0.3",
    "chalk": "^4.0.0",
    "dotenv": "^8.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "lighthouse": "^5.6.0",
    "lint-staged": "^10.1.7",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "react-test-renderer": "^16.13.1",
    "serve-handler": "^6.1.2",
    "storybook-readme": "^5.0.8",
    "typescript": "^3.8.3"
  },
  "lint-staged": {
    "*.{js,css,md}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*js",
      "!**/__snapshots__",
      "!src/**/*.story.js",
      "!**/serviceWorker.js",
      "!src/index.js"
    ]
  },
  "proxy": "http://localhost:5000",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
