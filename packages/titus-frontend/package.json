{
  "name": "titus-frontend",
  "version": "1.0.0-alpha.1",
  "private": true,
  "dependencies": {
    "auth0-js": "^9.11.3",
    "aws-amplify": "^1.2.4",
    "cross-env": "^6.0.3",
    "react": "^16.12.0",
    "react-adal": "^0.5.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "regenerator-runtime": "^0.13.3",
    "yup": "^0.27.0"
  },
  "scripts": {
    "build": "react-scripts build",
    "create:env": "node -r fs -e \"fs.copyFileSync('.env.sample', '.env', fs.constants.COPYFILE_EXCL)\"",
    "eject": "react-scripts eject",
    "lighthouse": "node lighthouse",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:staged": "lint-staged",
    "start": "react-scripts start",
    "storybook": "start-storybook -p 9009 -s public",
    "storybook:build": "build-storybook -s public",
    "test": "cross-env CI=true react-scripts test --env=jsdom --watchAll=false --coverage",
    "test:watch": "react-scripts test --env=jsdom",
    "test:related": "cross-env CI=true react-scripts test --env=jsdom --onlyChanged"
  },
  "lint-staged": {
    "*.{js,css,md}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*js",
      "!**/__snapshots__",
      "!src/**/*.story.js",
      "!**/serviceWorker.js",
      "!src/index.js"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@storybook/addon-a11y": "^5.2.6",
    "@storybook/addon-actions": "^5.2.6",
    "@storybook/addon-info": "^5.2.6",
    "@storybook/addon-knobs": "^5.2.6",
    "@storybook/addon-links": "^5.2.6",
    "@storybook/addon-options": "^5.2.6",
    "@storybook/addons": "^5.2.6",
    "@storybook/react": "^5.2.6",
    "@testing-library/react": "^9.3.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "formik": "^2.0.4",
    "lighthouse": "^5.6.0",
    "lint-staged": "^9.4.3",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "react-scripts": "^3.2.0",
    "react-test-renderer": "^16.12.0",
    "serve-handler": "^6.1.2",
    "storybook-readme": "^5.0.8"
  },
  "proxy": "http://localhost:5000",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
