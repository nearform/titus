{
  "name": "@nearform/titus-components",
  "version": "1.0.0",
  "description": "A library of React components for Titus",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component --no-demo --copy-files",
    "build:watch": "nodemon -w src -x 'npm run build'",
    "clean": "nwb clean-module && nwb clean-demo",
    "start": "nwb serve-react-demo",
    "test:nwb": "nwb test-react",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "@material-ui/core": "^1.1.0",
    "@material-ui/icons": "^1.1.0",
    "@nearform/react-table": "^1.0.1",
    "downshift": "^2.0.10",
    "nodemon": "^1.17.5"
  },
  "peerDependencies": {
    "@reach/router": "^1.1.1",
    "react": "16.x"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-runtime": "^6.26.0",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.9.1",
    "eslint-plugin-standard": "^3.1.0",
    "nwb": "0.21.x",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "license": "Apache-2.0",
  "repository": "nearform/titus",
  "keywords": []
}
