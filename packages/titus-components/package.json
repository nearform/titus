{
  "name": "@nearform/titus-components",
  "version": "1.1.0",
  "description": "A library of React components for Titus",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component --no-demo --copy-files",
    "build:watch": "nodemon -e js,ts -w src -x 'npm run build'",
    "clean": "nwb clean-module && nwb clean-demo",
    "start": "nwb serve-react-demo",
    "test": "jest --coverage --runInBand",
    "test:watch": "npm run test -- --watch --runInBand",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "prepublish": "npm run build",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "build:storybook:docs": "build-storybook -c .storybook -o ../../docs/components-storybook",
    "format": "prettier --write '**/*.js'"
  },
  "dependencies": {
    "@material-ui/core": "^3.3.0",
    "@material-ui/icons": "^3.0.1",
    "@nearform/commentami-react-components": "^1.0.4",
    "@nearform/react-table": "^1.0.6",
    "@types/reach__router": "^1.0.1",
    "aws-sdk": "^2.318.0",
    "downshift": "^2.0.10",
    "prop-types": "^15.6.2",
    "react-dnd": "^5.0.0",
    "react-dnd-html5-backend-filedrop": "^1.0.0"
  },
  "peerDependencies": {
    "react": "16.x"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.11",
    "@storybook/addon-knobs": "^3.4.11",
    "@storybook/addon-options": "^3.4.11",
    "@storybook/react": "^3.4.11",
    "babel-core": "^6.26.3",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "eslint": "^4.1.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.50.1",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "jest": "^23.4.1",
    "jest-dom": "^1.12.1",
    "jest-environment-jsdom": "^23.4.0",
    "jest-environment-jsdom-global": "^1.1.0",
    "lodash": "^4.17.10",
    "nodemon": "^1.17.5",
    "nwb": "0.21.x",
    "prettier": "^1.14.3",
    "react": "^16.5.2",
    "react-dnd-test-backend": "^5.0.1",
    "react-dom": "^16.5.2",
    "react-testing-library": "^4.0.2"
  },
  "license": "Apache-2.0",
  "repository": "nearform/titus",
  "keywords": []
}
